<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.hello" version="1.0.0"
    xmlns="http://www.w3.org/ns/widgets"
    xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Nông trại cổ tích</name>
    <description>Game Nông trại cổ tích</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        EG Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="https://farm.smavi.men" />
    <allow-navigation href="https://farm.smavi.men/*" />
    <platform name="android">
        <preference name="PLAY_SERVICES_VERSION" default="23.2.0" />
        <preference name="AndroidXEnabled" value="true" />
        <preference name="GradlePluginKotlinEnabled" value="true" />
    </platform>
</widget>
